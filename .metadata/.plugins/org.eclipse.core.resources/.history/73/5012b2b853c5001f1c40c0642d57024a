package MultipleClasses;

public class Stats {

	public Data data;
	public int sum;
	
	public Stats() {		
	}
	
	public Stats(Data data) {
		this.data = data;
	}
	
	public int calculateSum() {
		int[] ary = data.ary;
		sum = 0;
		for (int i = 0; i < ary.length; i++) {
			sum += ary[i];
		}
		
		return sum;
	}
	
}
