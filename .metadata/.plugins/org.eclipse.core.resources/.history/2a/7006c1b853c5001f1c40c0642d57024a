package MultiInterfaceClassesClone;

public class MainClass {
	
	public static void main(String[] args) {
		// Create data object d1 and display the array data.
		Data d1 = new Data(10);
		d1.displayAry(d1.ary);
		d1.displayStats(d1.ary);
		
		// Create data object d2 and display the array data.
		Data d2 = new Data(20);
		d2.displayAry(d2.ary);
		d2.displayStats(d2.ary);
		
		d1.sortData(d1.ary);
	}

}
