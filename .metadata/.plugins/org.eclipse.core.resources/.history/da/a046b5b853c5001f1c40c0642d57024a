package AbstractClasses;

public class ProcessClass extends WorkerClass {
	
	public int cnt;
	
	public ProcessClass(int cnt) {
		this.cnt = cnt;
	}

// Completing the abstract method defined in the WorkerClass
	public void start() {
		int i = 0;
		
		while (i < cnt) {
			System.out.println ("Executing the start method. Cnt = " + i);
			i++;
		}
	}
	
}